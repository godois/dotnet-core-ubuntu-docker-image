    --name simple-dotnet-app \
    --hostname="dotnet.example.com" \
    godois/dotnet-core-ubuntu:1.0;docker logs simple-dotnet-app

docker exec -i -t simple-dotnet-app /bin/bash

docker logs simple-dotnet-app


